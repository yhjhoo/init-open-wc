{
  "name": "app-wc-storybook",
  "description": "Webcomponent app-wc-storybook following open-wc recommendations",
  "license": "MIT",
  "author": "app-wc-storybook",
  "version": "0.0.0",
  "scripts": {
    "storybook": "npm run analyze -- --exclude dist && web-dev-server -c .storybook/server.mjs",
